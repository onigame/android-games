<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
	package="com.google.android.games.tubes">
	<application android:icon="@drawable/icon" android:label="@string/app_name">
		<activity android:name=".PlayTubes">
			<intent-filter>
				<action android:name="android.intent.action.MAIN" />
				<category android:name="android.intent.category.LAUNCHER" />
			</intent-filter>
		</activity>
	  <activity android:name=".ScoresActivity" android:stateNotNeeded="false">
      <intent-filter>
        <action android:name="android.intent.action.INSERT" />
        <category android:name="android.intent.category.DEFAULT" />
      </intent-filter>
    </activity>
    <provider android:name="ScoresProvider" android:authorities="com.google.android.games.tubes.ScoreList" />
  </application>
</manifest>
